				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "srcfilename",
					"value": "nyc_data"
				}
			},
			{
				"name": "SetContainerName",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "srcfilecontainer",
					"value": "nycip"
				}
			},
			{
				"name": "NycDataCopy",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "SetFilename",
						"dependencyConditions": [
							"Succeeded"
						]
					},
