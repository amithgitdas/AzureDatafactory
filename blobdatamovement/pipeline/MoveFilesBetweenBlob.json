							},
							{
								"source": {
									"type": "String",
									"ordinal": 4
								},
								"sink": {
									"path": "$['dropoffdate']"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 5
								},
								"sink": {
									"path": "$['dropoff_hour']"
								}
							}
						]
					}
				},
